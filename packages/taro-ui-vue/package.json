{
	"name": "@lexmin0412/taro-ui-vue",
	"version": "2.0.0-beta.0",
	"description": "UI KIT for Taro/vue",
	"browser": "src/index.js",
	"module": "src/index.js",
	"main": "src/index.js",
	"source": "src/index.js",
	"files": [
		"dist",
		"src"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"author": "psaren",
	"repository": {
		"type": "git",
		"url": "git@github.com:psaren/taro-ui-vue.git"
	},
	"templateInfo": {
		"name": "vuex",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"test:analyze": "rollup --config ./config/rollup.config.analyze.js",
		"build:lib": "tsc --project ./tsconfig.build.json",
		"build:lib-dev": "cross-env LIB_ENV=dev rollup --config ./config/rollup.config.js",
		"build:rollup": "rollup --config ./config/rollup.config.js && node config/copy.js",
		"rollup:test": "node tests/transform-component.js && rollup --config ./config/rollup-test.config.js",
		"clean": "rimraf dist lib",
		"clog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"test": "node ./tests/transform-component.js && cross-env NODE_ENV=test && jest --coverage",
		"lint": "eslint ./src/**/*.{ts,vue}",
		"lint:fix": "eslint ./src/**/*.{ts,vue} --fix"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"license": "MIT",
	"dependencies": {
		"classnames": "^2.2.6",
		"dayjs": "^1.8.27",
